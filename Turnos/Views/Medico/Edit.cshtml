@model Turnos.Models.MedicoModel

@{
    ViewData["Title"] = "Edit";
}
<partial name="~/Views/Shared/_ValidateSession.cshtml" />
<h4>Médico</h4>
<div class="divider grey darken-2" tabindex="-1"></div>
<div class="row pb-5">
    <div class="col s12 m12 l12">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly"></div>
            <input type="hidden" asp-for="IdMedico" />
            <partial name="_Formulario" />
            <div class="input-field btn-center">
                <button class="btn waves-effect waves-light acento cian-4" type="submit" name="action">
                    Editar
                    <i class="material-icons left">how_to_reg</i>
                </button>
                <a asp-action="Index" class="waves-effect waves-light btn-small grey darken-4"><i class="material-icons left">undo</i>Volver al Listado</a>
            </div>
        </form>
    </div>
</div>
